# For real-time bot (npm run start)
TELEGRAM_BOT_TOKEN=your_bot_token_from_botfather

# For historical fetch (npm run fetch) - get from https://my.telegram.org/apps
TELEGRAM_API_ID=your_api_id
TELEGRAM_API_HASH=your_api_hash

# Required for both
TELEGRAM_GROUP_ID=your_group_id

# OCR crop region coordinates (adjust based on your camera's text position)
OCR_CROP_X=100
OCR_CROP_Y=50
OCR_CROP_WIDTH=200
OCR_CROP_HEIGHT=100
